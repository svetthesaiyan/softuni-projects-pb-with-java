package conditional_statement_exercise;

import java.util.Scanner;

public class c_speed_info 
{
	public static void main(String[] args) 
	{
		Scanner scanner=new Scanner(System.in);
		double money=Double.parseDouble(scanner.nextLine())*100;
		
		
		if(money>=0.00 && money<=9.99)
		{
		double moneyWhole=(money/100)%10;
		System.out.printf("%.0f %n", Math.floor(moneyWhole));
		
		double moneyTenths=(money/10)%10;
		System.out.printf("%.0f %n", Math.round(moneyTenths));
		
		double moneyHundredths=money%10;
		System.out.printf("%.0f %n", moneyHundredths);
		}
		else if(money>=10.00 && money<=99.99)
		{
		double moneyWhole=(money/100)%10;
		System.out.printf("%.0f %n", Math.floor(moneyWhole));
			
		double moneyTenths=(money/10)%10;
		System.out.printf("%.0f %n", moneyTenths);
			
		double moneyHundredths=money%10;
		System.out.printf("%.0f %n", moneyHundredths);
		}
	}
}
