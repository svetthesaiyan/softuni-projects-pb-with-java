package conditional_statement_exercise;

public class c_speed_info 
{
	public static void main(String[] args) 
	{
		double money=123;
		
		double moneyWhole=(money/100)%10;
		System.out.printf("%.0f %n", moneyWhole);
		
		double moneyTenths=(money/10)%10;
		System.out.printf("%.0f %n", moneyTenths);
		
		double moneyHundredths=money%10;
		System.out.printf("%.0f %n", moneyHundredths);
	}
}
