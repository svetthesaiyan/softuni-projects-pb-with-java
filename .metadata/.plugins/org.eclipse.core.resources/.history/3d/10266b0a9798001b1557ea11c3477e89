package first_steps_in_programming_extra_exercises;

import java.util.Scanner;

public class f_fish_market 
{
	public static void main(String[] args) 
	{
		Scanner scanner=new Scanner(System.in);
		double mackerel_price=Double.parseDouble(scanner.nextLine());
		double sprat_price=Double.parseDouble(scanner.nextLine());
		double mussels_price=7.50;
		
		double bonito_weight=Double.parseDouble(scanner.nextLine());
		double scad_weight=Double.parseDouble(scanner.nextLine());
		int mussels_weight=Integer.parseInt(scanner.nextLine());
		
		
 
	}
}
